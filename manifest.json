{
  "manifest_version": 3,
  "name": "Price to Time Converter",
  "version": "1.0",
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "action": { "default_popup": "popup.html" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}