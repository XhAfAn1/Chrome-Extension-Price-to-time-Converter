{
  "manifest_version": 3,
  "name": "Price to Time Converter",
  "description": "Convert product prices into time worked based on your income.",
  "version": "1.0",
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
 "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
